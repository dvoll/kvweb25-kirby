{
    "name": "dvll/kvweb25-kirby",
    "description": "KV Website",
    "homepage": "https://cvjm-stift-quernheim.de",
    "type": "project",
    "keywords": [
        "kirby",
        "cms",
        "website"
    ],
    "authors": [
        {
            "name": "Dario Voll",
            "homepage": "https://daario.de"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./plugins/*"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": ">=8.3",
        "getkirby/cms": "^5.0.0",
        "getkirby/cli": "^1.4",
        "arnoson/kirby-vite": "^5.5",
        "tobimori/kirby-seo": "^2.0",
        "vlucas/phpdotenv": "^5.6",
        "dvll/sitepackage": "@dev",
        "dvll/kirby-events": "@dev",
        "johannschopplich/kirbylog": "^1.4",
        "bnomei/kirby3-security-headers": "^5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.54",
        "phpstan/phpstan": "^1.10",
        "lukaskleinschmidt/kirby-types": "^2.0"
    }
}
